{
  "version": 1,
  "domain": "app.lfg.directory",
  "display_name": "LFG Directory",
  "username": {
    "re": "^[a-zA-Z0-9_@+.]{2,150}$",
    "min": 2,
    "max": 150
  },
  "brand_color": "#1779ba",
  "logo": {
    "full_color": "https://app.lfg.directory/static/images/full_color.svg",
    "small_black": "https://app.lfg.directory/static/images/small_black.svg"
  },
  "description": "A tabletop RPG campaign manager and group finder.",
  "prefill_url": "https://app.lfg.directory/keybase-proofs/new-proof?kb_username=%{kb_username}&username=%{username}&sig_hash=%{sig_hash}&kb_ua=%{kb_ua}",
  "profile_url": "https://app.lfg.directory/keybase-proofs/profile/%{username}",
  "check_url": "https://app.lfg.directory/keybase-proofs/api/%{username}",
  "check_path": ["keybase_sigs"],
  "contact": ["daniel@mg.lfg.directory", "joshblum@keybase"]
}
